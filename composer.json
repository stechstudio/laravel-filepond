{
    "name": "stechstudio/laravel-upload-server",
    "description": "Laravel backend for handling FilePond uploads with large file chunking support",
    "keywords": [
        "stechstudio",
        "laravel",
        "upload",
        "filepond"
    ],
    "homepage": "https://github.com/stechstudio/laravel-upload-server",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "pion/laravel-chunk-upload": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "STS\\UploadServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\UploadServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\UploadServer\\UploadServerServiceProvider"
            ],
            "aliases": {
                "UploadServer": "STS\\UploadServer\\UploadServerFacade"
            }
        }
    }
}
